public class  {
    public static void main(String[] args) {
        boolean b = true;
        char c = 'a';
        int i = 12554566;
        float f = 5.85f;
        double d = 10.365;
        
        System.out.println("boolean b = " + b);
        System.out.println("char c = " + c);
        System.out.println("int i = " + i);
        System.out.println("float f = " + f);
        System.out.println("double d = " + d);
    }
}
